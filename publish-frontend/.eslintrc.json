{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "unused-imports"
  ],
  "rules": {
    "no-unused-vars": 2,
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "error",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}