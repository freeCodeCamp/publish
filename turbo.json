{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "build/**"],
      "dotEnv": [".env.local", ".env"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "test": {},
    "lint": {},
    "develop": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
