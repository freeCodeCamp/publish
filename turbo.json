{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "build/**"],
      "dotEnv": [".env.local", ".env"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "test": {},
    "lint": { "dependsOn": ["prettier-check", "eslint"] },
    "prettier-check": {},
    "eslint": {},
    "develop": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "seed": { "cache": false, "dependsOn": ["init"] },
    "init": { "cache": false }
  }
}
